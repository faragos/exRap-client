{
  "name": "client-exrap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@fullcalendar/timegrid": "^5.5.1",
    "@material-ui/core": "^5.0.0-alpha.30",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^5.0.0-alpha.30",
    "@reduxjs/toolkit": "^1.5.0",
    "@rtk-incubator/rtk-query": "^0.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "date-fns": "^2.20.3",
    "jwt-decode": "^3.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "~4.1.5",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "prepare": "git config core.hooksPath ./git-hooks",
    "pre-push": "npm run lint && cross-env CI=true npm run test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "generate-example-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file generated.api.ts --hooks https://petstore3.swagger.io/api/v3/openapi.json --baseUrl https://petstore3.swagger.io/api/v3",
    "generate-time-track-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file ./src/gen/timeTrack.api.generated.ts --baseQuery ./src/service/apiBaseQuery.ts:apiBaseQuery --hooks ./api/TimeTrackAPI.json --reducerPath timeTrackApi",
    "generate-dashboard-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file ./src/gen/dashboard.api.generated.ts --baseQuery ./src/service/apiBaseQuery.ts:apiBaseQuery --hooks ./api/DashAPI.json --reducerPath dashboardApi",
    "generate-auth-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file ./src/gen/auth.api.generated.ts --baseQuery ./src/service/apiBaseQuery.ts:apiBaseQuery --hooks ./api/AuthAPI.json --reducerPath authApi",
    "generate-api": "npm run generate-time-track-api -- --baseUrl https://localhost/time && npm run generate-dashboard-api -- --baseUrl https://localhost/auth && npm run generate-auth-api -- --baseUrl https://localhost/dash",
    "generate-api-production": "npm run generate-time-track-api -- --baseUrl https://petstore3.swagger.io/api/v3 && npm run generate-dashboard-api -- --baseUrl https://petstore3.swagger.io/api/v3 && npm run generate-auth-api -- --baseUrl https://petstore3.swagger.io/api/v3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rtk-incubator/rtk-query-codegen-openapi": "^0.2.4",
    "@types/jsonwebtoken": "^8.5.1",
    "@typescript-eslint/eslint-plugin": "~4.17.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest-sonar-reporter": "^2.0.0",
    "jsonwebtoken": "^8.5.1"
  }
}
