    "@fullcalendar/timegrid": "^5.5.1",
    "@material-ui/core": "^4.11.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@rtk-incubator/rtk-query": "^0.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "~4.1.5",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-example-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file generated.api.ts --hooks https://petstore3.swagger.io/api/v3/openapi.json --baseUrl https://petstore3.swagger.io/api/v3",
    "generate-time-track-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file ./src/gen/timeTrack.api.generated.ts --hooks ./api/TimeTrackAPI.json",
    "generate-dashboard-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file ./src/gen/dashboard.api.generated.ts --hooks ./api/DashApi.json",
    "generate-auth-api": "npx @rtk-incubator/rtk-query-codegen-openapi --file ./src/gen/auth.api.generated.ts --hooks ./api/AuthApi.json",
    "generate-api": "npm run generate-time-track-api -- --baseUrl https://localhost:5001 && npm run generate-dashboard-api -- --baseUrl https://localhost:5001 && npm run generate-auth-api -- --baseUrl https://localhost:5001",
    "generate-api-production": "npm run generate-time-track-api -- --baseUrl https://petstore3.swagger.io/api/v3 && npm run generate-dashboard-api -- --baseUrl https://petstore3.swagger.io/api/v3 && npm run generate-auth-api -- --baseUrl https://petstore3.swagger.io/api/v3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~4.17.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest-sonar-reporter": "^2.0.0"
  }
}
